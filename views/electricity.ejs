      <div class="field">
        <label>Semester</label>
        <input placeholder="5" name="semester" type="text">
      </div>
    </div>
    
    
     <div class="two fields">
      <div class="field">
        <label>Phone No.</label>
        <input placeholder="9456358759" name="phone_no" type="text">
      </div>
      <div class="field">
        <label>USN</label>
        <input placeholder="1by16cs038" name="usn" type="text">
      </div>
    </div>
    
    
    <div class="two fields">
      <div class="field">
        <label>Hostel Block & Room No.</label>
        <input placeholder="A-110" name = "block_room_no" type="text">
      </div>
      <div class="field">
        <label>Email Id</label>
        <input placeholder="xyz@gmail.com" name="email" type="text">
      </div>
    </div>
    
     <div class="fields">
        <label>Complaint Subject</label>
        <input placeholder="Non-Functional Tubelight" name="subject" type="text">
      </div>
    <div class="field">
    <label></label>
    <textarea name="description" placeholder="Description..."></textarea>
  </div>
    
    <div class="inline field">
      
    </div>
