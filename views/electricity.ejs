<div class="ui inverted segment">
  <form class="ui inverted form" action="/complaints/electricity" method="POST">
      <h1>ELECTRICITY COMPLAINT FORM</h1>
    <div class="two fields">
      <div class="field">
        <label>First Name</label>
        <input placeholder="Jatin" name="first_name" type="text">
      </div>
      <div class="field">
        <label>Last Name</label>
        <input placeholder="Sohane" name="last_name" type="text">
      </div>
    </div>
    
    
    <div class="two fields">
      <div class="field">
        <label>Branch</label>
        <input placeholder="CSE" name="branch"  type="text">
